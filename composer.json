{
    "name": "rappasoft/laravel-5-boilerplate",
    "description": "The Laravel Boilerplate Project.",
    "keywords": ["framework", "laravel", "boilerplate"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "arcanedev/log-viewer": "^4.4",
        "arcanedev/no-captcha": "~3.0",
        "backpack/MenuCRUD": "^1.0",
        "backpack/base": "^0.8.2",
        "backpack/crud": "^3.3",
        "backpack/langfilemanager": "^1.0",
        "backpack/pagemanager": "^1.1",
        "backpack/settings": "^2.0",
        "creativeorange/gravatar": "~1.0",
        "davejamesmiller/laravel-breadcrumbs": "^4.1",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "google/apiclient": "^2.2",
        "hieu-le/active": "^3.5",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.5.*",
        "spatie/laravel-backup": "^5.1",
        "spatie/laravel-html": "^2.4",
        "spatie/laravel-permission": "^2.5",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "backpack/generators": "^1.1",
        "barryvdh/laravel-debugbar": "^3.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "dev-master",
        "laravel/browser-kit-testing": "^2.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
      "clear-all": [
        "composer dumpautoload -o",
        "@php artisan optimize",
        "@php artisan clear-compiled",
        "@php artisan cache:clear",
        "@php artisan route:clear",
        "@php artisan view:clear",
        "@php artisan config:clear"
      ],
      "phpunit": [
        "phpunit > output.txt -d memory_limit=2048M"
      ]      ,
      "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "test" : [
            "vendor/bin/phpunit > phpunit.txt"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "minimum-stability": "dev",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
